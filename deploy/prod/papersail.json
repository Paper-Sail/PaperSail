{
  "cpus": 0.1,
  "mem": 512,
  "container": {
    "type": "MESOS",
    "docker": {},
     "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 0,
        "name": "http"
      }
    ]
  },
  "env": {
    "VERSION": "_VERSION_",
    "SENTRY_ENV": "prod",
    "SENTRY_DSN": "_SENTRY_DSN_",
    "SENTRY_PUBLIC_DSN": "_SENTRY_PUBLIC_DSN_",
    "ENABLE_FLUENTD_LOGGING": "true"
  },
  "healthChecks": [
    {
      "path": "_HC_PATH_",
      "protocol": "MESOS_HTTP",
      "port": 80,
      "gracePeriodSeconds": 360,
      "intervalSeconds": 10,
      "timeoutSeconds": 10,
      "maxConsecutiveFailures": 3
    }
  ],
  "labels": {
    "HAPROXY_GROUP": "external",
    "HAPROXY_0_ENABLED": true,
    "HAPROXY_0_REDIRECT_TO_HTTPS": true,
    "HAPROXY_0_VHOST": "_VHOST_"
  },
  "networks": [
    {
      "mode": "container",
      "name": "asl"
    }
  ],
  "constraints": [
    [
      "group",
      "IS",
      "external"
    ]
  ]
}
