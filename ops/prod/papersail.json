{
  "id": "/arte/veryveryshort/prod/papersail",
  "cpus": 0.1,
  "mem": 512,
  "container": {
    "type": "MESOS",
    "volumes": [
    ],
    "docker": {}
  },
  "env": {
  },
  "healthChecks": [
    {
      "path": "/",
      "protocol": "HTTP",
      "port": 5000,
      "gracePeriodSeconds": 45,
      "intervalSeconds": 10,
      "timeoutSeconds": 10,
      "maxConsecutiveFailures": 3
    }
  ],
  "labels": {
    "HAPROXY_GROUP": "external",
    "HAPROXY_0_REDIRECT_TO_HTTPS": true,
    "HAPROXY_0_VHOST": "papersail.lab.arte.tv"
  },
  "portDefinitions": [],
  "ipAddress": {
    "discovery": {
      "ports": [
        {
          "number": 5000,
          "name": "http",
          "protocol": "tcp",
          "labels": {}
        }
      ]
    },
    "networkName": "vlan"
  }
}
